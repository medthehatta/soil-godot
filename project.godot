; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "BackendRequest",
"language": "GDScript",
"path": "res://Scripts/BackendRequest.gd"
}, {
"base": "Reference",
"class": "BackendRequestManager",
"language": "GDScript",
"path": "res://Scripts/BackendRequestManager.gd"
}, {
"base": "Reference",
"class": "Rng",
"language": "GDScript",
"path": "res://Scripts/Rng.gd"
} ]
_global_script_class_icons={
"BackendRequest": "",
"BackendRequestManager": "",
"Rng": ""
}

[application]

config/name="Soil 1"
run/main_scene="res://Scenes/Main.tscn"

[autoload]

Utilities="*res://Scripts/Utilities.gd"
ControlUtil="*res://Scripts/ControlUtil.gd"
State="*res://Scripts/State.gd"
Model="*res://Scripts/Model.gd"

[display]

window/size/width=576
window/size/height=1024
window/size/test_width=300
window/size/test_height=533
window/handheld/orientation="portrait"
window/stretch/mode="2d"

[rendering]

quality/driver/driver_name="GLES2"
quality/2d/use_pixel_snap=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
quality/shading/force_vertex_shading=true
